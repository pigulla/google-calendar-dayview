{
    "extends": [
        "four66"
    ],

    "env": {
        "node": true
    },

    "plugins": [
        "react"
    ],

    "ecmaFeatures": {
        "jsx": true
    },

    "parser": "babel-eslint",

    "parserOptions": {
        "ecmaVersion": 2017,
        "sourceType": "module"
    },

    "rules": {
        "no-mixed-operators": "off"
    }
}
