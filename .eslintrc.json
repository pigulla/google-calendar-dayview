{
    "extends": [
        "four66",
        "plugin:inferno/recommended"
    ],

    "env": {
        "node": true
    },

    "plugins": [
        "inferno"
    ],

    "ecmaFeatures": {
        "jsx": true
    },

    "parser": "babel-eslint",

    "parserOptions": {
        "ecmaVersion": 2017,
        "sourceType": "module"
    },

    "rules": {
        "camelcase": "off"
    }
}
