{
  "name": "google-calendar-dayview",
  "version": "0.4.0",
  "description": "A dead simple dashboard for meeting rooms using Google Calendar",
  "main": "dist/backend/cli/index.js",
  "engines": {
    "node": ">=8"
  },
  "scripts": {
    "test": "npm run lint && npm run test-frontend && npm run nsp",
    "lint": "eslint src",
    "nsp": "nsp check",
    "test-frontend": "mocha --require babel-register --require test/setup.js --recursive test/frontend/**/*.spec.js",
    "compile-backend": "babel --out-dir dist/ --ignore src/frontend/ src/ ",
    "compile-frontend": "NODE_ENV=production webpack",
    "eslint": "eslint src/",
    "prepublishOnly": "npm test && rm -rf dist/ && npm run compile-backend && npm run compile-frontend",
    "start": "node dist/backend/cli/index.js"
  },
  "files": [
    "dist/"
  ],
  "bin": "./dist/backend/cli/index.js",
  "author": {
    "name": "Raphael Pigulla",
    "email": "pigulla@four66.com",
    "url": "http://www.n3rd.org"
  },
  "license": "ISC",
  "keywords": [
    "google-api",
    "google-calendar",
    "calendar",
    "dashboard",
    "room"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/pigulla/google-calendar-dayview.git"
  },
  "bugs": {
    "url": "https://github.com/pigulla/google-calendar-dayview/issues"
